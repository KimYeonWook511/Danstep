<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.danstep.model.mapper.ScoreMapper">

    <insert id="insertScore" parameterType="com.danstep.model.dto.Score">
        INSERT INTO Score (bad, good, great, perfect, health)
        VALUES (#{bad}, #{good}, #{great}, #{perfect}, #{health})
    </insert>

    <select id="getScore" parameterType="com.danstep.model.dto.Score">
        SELECT * FROM score;
    </select>

</mapper>